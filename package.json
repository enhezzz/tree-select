  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeromake/zero-tree/issues"
  },
  "keywords": [
    "vue",
    "tree",
    "vue-tree"
  ],
  "files": [
    "index.js",
    "LICENSE",
    "README.md",
    "dist"
  ],
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.4.3",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.0.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-vue": "^2.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "ghooks": "^2.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "validate-commit-msg": "^2.12.2",
    "vue-hot-reload-api": "^2.1.0",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.1",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "vue": "^2.3.0"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
