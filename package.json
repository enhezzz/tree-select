{
  "name": "zero-tree",
  "version": "0.1.3",
  "description": "A Vue Tree by zeromake",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeromake/zero-tree"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --watch --progress --colors --config build/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --colors --config build/webpack.config.js",
    "dev_build": "cross-env NODE_ENV=development ANALY=dev webpack --progress --colors --config build/webpack.config.js"
